package sim;

import sim.operation.DefaultFileManager;

public class AverageRuns {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		if (args.length != 2){
			System.out.println("wrong number of args!");
			System.exit(0);
		}
		String simfolder = args[0];
		String outfolder = args[1];
		DefaultFileManager.getAveragesOfSimulationRuns(simfolder, outfolder);
		

	}

}
